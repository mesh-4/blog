{
  "name": "blog",
  "version": "0.8.0",
  "main": "index.js",
  "repository": "https://github.com/senlima0430/blog.git",
  "author": "Senlima Sun <senlima0430@gmail.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://senlima.blog",
  "scripts": {
    "start": "concurrently \"npm run watch:css\" \"PORT=3500 craco start\"",
    "build": "yarn build:css && PUBLIC_URL=https://senlima.blog craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/css/source.css -o src/css/main.css -w",
    "build:css": "postcss src/css/source.css -o src/css/main.css --env production"
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "core-js": "^3.6.5",
    "firebase": "^7.18.0",
    "history": "^5.0.0",
    "immer": "^7.0.7",
    "markdown-it": "^11.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-h5-audio-player": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-is": "^16.13.1",
    "react-lines-ellipsis": "^0.14.1",
    "react-markdown": "^4.3.1",
    "react-markdown-editor-lite": "^1.2.2",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.3",
    "react-share": "^4.2.1",
    "react-snap": "^1.23.0",
    "react-snapshot": "^1.3.0",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^13.3.1",
    "react-toastify": "^6.0.8",
    "recoil": "^0.0.10",
    "typewriter-effect": "^2.13.1",
    "use-immer": "^0.4.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "craco-alias": "^2.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "source": "build",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "exclude": [
      "/login",
      "/dashboard/**"
    ],
    "snapshotDelay": 1000
  }
}
